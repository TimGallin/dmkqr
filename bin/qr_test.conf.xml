<!-->
Implicit funcstion:
f_RSA(plaintext,keyfile): RSA encrpt

$ ï¼šref var(implicit or explicit)

Implicit vars:
tmstamp10:unix stamp,Second precision
tmstamp13:time stamp,Millisecond precision
<-->

<conf>
    <implicitpreset rsakey="config/rsa.cert.key"></implicitpreset>

    <scheme name="smkp">
        <variable name="code" ui="editbox">
            <preset>12345678</preset>
            <preset>kij78772</preset>
        <variable>
        
        <variable name="deskey" ui="editbox">
            <preset>kjlkjijlk</preset>
            <preset>asdd44fgt</preset>
        <variable>

        <implicitpreset deskey="config/des.key.txt"></implicitpreset>

        <formula>.f_RSA($code + .$tmstamp10 + .f_DES(deskey), "config/rsa.cert.txt")</formula>
    </scheme>
</conf>
